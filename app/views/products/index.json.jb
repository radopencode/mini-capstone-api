@products.map do |product|
  {
  id: product.id,
  name: product.name,
  images: product.images, 
  price: product.price,
  description: product.description,
  id: product.id,
  url: product_url(product, format: :json)
  is_discouted?: product.is is_discouted?,
  tax: product.tax,
  total: product.total
  supplier: product.supplier
  }
end
